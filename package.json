{
  "name": "sample-api",
  "version": "0.0.0",
  "description": "Sample api for postgreSQL",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "test": "jest",
    "start": "node dist/server.js",
    "db:start": "docker run --name some-postgres --rm -e POSTGRES_PASSWORD=postgres -p 5432:5432 -d postgres",
    "db:stop": "docker stop some-postgres",
    "db:init": "ts-node src/db-init.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bethanypage/sample-api.git"
  },
  "keywords": [
    "Node",
    "Express",
    "Typescript"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bethanypage/sample-api/issues"
  },
  "homepage": "",
  "devDependencies": {
    "@types/express": ">=4.17.x",
    "@types/jest": ">=26.0.x",
    "@types/node": ">=14.14.x",
    "@types/pg": "^8.6.0",
    "@types/supertest": ">=2.x",
    "jest": ">=26.5.x",
    "nodemon": "^2.0.7",
    "prettier": "2.3.0",
    "supertest": ">=6.1.x",
    "testcontainers": "^7.18.1",
    "ts-jest": ">=26.5.x",
    "ts-node": ">=8.10.x",
    "typescript": ">=4.2.x"
  },
  "dependencies": {
    "@cloudsense/cs-logger": "^4.0.1",
    "express": "^4.17.1",
    "pg": "^8.6.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testTimeout": 30000
  }
}
